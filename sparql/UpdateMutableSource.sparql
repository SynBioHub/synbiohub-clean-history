PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX sbh: <http://wiki.synbiohub.org/wiki/Terms/synbiohub#>

DELETE WHERE {
    <$topLevel> sbh:mutableProvenance ?source .
}
;
DELETE WHERE {
    <$topLevel> dcterms:modified ?modified .
}
;
INSERT DATA {
    <$topLevel> sbh:mutableProvenance $source .
    <$topLevel> dcterms:modified $modified .
} 


